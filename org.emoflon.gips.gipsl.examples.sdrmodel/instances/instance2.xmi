<?xml version="1.0" encoding="UTF-8"?>
<org.emoflon.gips.gipsl.examples.sdrmodel:Root
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:org.emoflon.gips.gipsl.examples.sdrmodel="platform:/resource/org.emoflon.gips.gipsl.examples.sdrmodel/model/Sdrmodel.ecore">
  <cpus name="cpu0">
    <threads
        name="thread0"
        processResource="2"/>
    <threads
        name="thread1"
        processResource="2"/>
    <intercoms
        name="t0to1"
        start="//@cpus.0/@threads.0"
        target="//@cpus.0/@threads.1"/>
    <intercoms
        name="t1to0"
        start="//@cpus.0/@threads.1"
        target="//@cpus.0/@threads.0"/>
  </cpus>
  <jobs name="job0">
    <blocks
        name="block0"
        outputs="//@jobs.0/@flows.0"
        processDemand="1"/>
    <blocks
        name="block1"
        inputs="//@jobs.0/@flows.0"
        outputs="//@jobs.0/@flows.1"
        processDemand="1"/>
    <blocks
        name="block2"
        inputs="//@jobs.0/@flows.1"
        outputs="//@jobs.0/@flows.2"
        processDemand="1"/>
    <blocks
        name="block3"
        inputs="//@jobs.0/@flows.2"
        processDemand="1"/>
    <flows name="flow0to1"
        rate="1"
        start="//@jobs.0/@blocks.0"
        target="//@jobs.0/@blocks.1"/>
    <flows name="flow1to2"
        rate="1"
        start="//@jobs.0/@blocks.1"
        target="//@jobs.0/@blocks.2"/>
    <flows name="flow2to3"
        rate="1"
        start="//@jobs.0/@blocks.2"
        target="//@jobs.0/@blocks.3"/>
  </jobs>
</org.emoflon.gips.gipsl.examples.sdrmodel:Root>
